#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/jspm@3.3.3/node_modules/jspm/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/jspm@3.3.3/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/jspm@3.3.3/node_modules/jspm/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/jspm@3.3.3/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jspm/jspm.js" "$@"
else
  exec node  "$basedir/../jspm/jspm.js" "$@"
fi
