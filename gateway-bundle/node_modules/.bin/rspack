#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/@rspack+cli@1.0.7_@rspack+core@1.0.7_@swc+helpers@0.5.13__@types+express@4.17.21_webpack@5.95.0/node_modules/@rspack/cli/bin/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/@rspack+cli@1.0.7_@rspack+core@1.0.7_@swc+helpers@0.5.13__@types+express@4.17.21_webpack@5.95.0/node_modules/@rspack/cli/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/@rspack+cli@1.0.7_@rspack+core@1.0.7_@swc+helpers@0.5.13__@types+express@4.17.21_webpack@5.95.0/node_modules/@rspack/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/@rspack+cli@1.0.7_@rspack+core@1.0.7_@swc+helpers@0.5.13__@types+express@4.17.21_webpack@5.95.0/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/@rspack+cli@1.0.7_@rspack+core@1.0.7_@swc+helpers@0.5.13__@types+express@4.17.21_webpack@5.95.0/node_modules/@rspack/cli/bin/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/@rspack+cli@1.0.7_@rspack+core@1.0.7_@swc+helpers@0.5.13__@types+express@4.17.21_webpack@5.95.0/node_modules/@rspack/cli/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/@rspack+cli@1.0.7_@rspack+core@1.0.7_@swc+helpers@0.5.13__@types+express@4.17.21_webpack@5.95.0/node_modules/@rspack/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/@rspack+cli@1.0.7_@rspack+core@1.0.7_@swc+helpers@0.5.13__@types+express@4.17.21_webpack@5.95.0/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@rspack/cli/bin/rspack" "$@"
else
  exec node  "$basedir/../@rspack/cli/bin/rspack" "$@"
fi
