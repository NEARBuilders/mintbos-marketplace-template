#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/bos-workspace@1.0.0-alpha.37_encoding@0.1.13/node_modules/bos-workspace/bin/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/bos-workspace@1.0.0-alpha.37_encoding@0.1.13/node_modules/bos-workspace/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/bos-workspace@1.0.0-alpha.37_encoding@0.1.13/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/bos-workspace@1.0.0-alpha.37_encoding@0.1.13/node_modules/bos-workspace/bin/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/bos-workspace@1.0.0-alpha.37_encoding@0.1.13/node_modules/bos-workspace/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/bos-workspace@1.0.0-alpha.37_encoding@0.1.13/node_modules:/Users/ebraem/workspace/near/nearbuilders/blackdragon-marketplace-bos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bos-workspace/bin/bw.js" "$@"
else
  exec node  "$basedir/../bos-workspace/bin/bw.js" "$@"
fi
